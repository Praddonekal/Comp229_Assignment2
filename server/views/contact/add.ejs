<%- include ../partials/header.ejs %>

<%- include ../partials/main_nav.ejs %>


<link rel='stylesheet' href='/Content/title_block.css' />

<!-- services page -->
<div class="services-section">
  <h2>ADD CONTACT</h2>
  <p>ENTER A NEW CONTACT INFORMATION</p>
</div>

<div class="container">
    <div class="row">
        <div class="offset-md-3 col-md-6">
<br><br>

        <form class="form" method="POST">
            <div class="form-group">
                <label for="nameTextField">Name of Contact</label>
                <input type="text" class="form-control"
                id="nameTextField"
                placeholder="Enter the Name of the Contact"
                name="name" required>
            </div>

            <div class="form-group">
                <label for="numberTextField">Number</label>
                <input type="text" class="form-control"
                id="numberTextField"
                placeholder="Enter the Number of the Contact"
                name="number" required>
            </div>

            <div class="form-group">
                <label for="emailTextField">E-mail</label>
                <input type="text" class="form-control"
                id="emailTextField"
                placeholder="Enter the E-mail of the Contact"
                name="email" required>
            </div>

            <button class="btn btn-primary" type="submit">
                </i> Submit</button>

            <a href="/contact-list" class="btn btn-warning">
                </i> Cancel</a>

        </form>
    </div>
    </div>
</div>
<br><br><br><br><br>



<%- include ../partials/bottom_nav.ejs %>
